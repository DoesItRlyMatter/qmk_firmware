BOOTMAGIC_ENABLE = no      # Enable Bootmagic Lite
MOUSEKEY_ENABLE = yes       # Mouse keys
EXTRAKEY_ENABLE = yes       # Audio control and System control
COMMAND_ENABLE = no       	# Commands for debug and configuration
NKRO_ENABLE = no            # Enable N-Key Rollover
BACKLIGHT_ENABLE = no      	# Enable keyboard backlight functionality
RGBLIGHT_ENABLE = no        # Enable RGB underglow
ENCODER_ENABLE = no			# Enable encoder
ENCODER_MAP_ENABLE = no		# Enable encoder mapping
AUDIO_ENABLE = no           # Audio output

# Enabled while debugging, can be disabled later.
CONSOLE_ENABLE = no

# Slower & smaller compile.
LTO_ENABLE = yes

# Deffered exec for jiggler.
DEFERRED_EXEC_ENABLE = yes

# WPM for bongo animation.
WPM_ENABLE = yes

# Either split or oled, both cannot be used at same time.
SPLIT_KEYBOARD = no
OLED_ENABLE = yes
